package ee.lps.dto;

public class BeaconMeasurementDTO {
    private String mac;
    private int rssi;

    public BeaconMeasurementDTO() {}

    public BeaconMeasurementDTO(String mac, int rssi) {
        this.mac = mac;
        this.rssi = rssi;
    }

    public BeaconMeasurementDTO(String mac) {
        this.mac = mac;
    }

    public String getMac() {
        return mac;
    }

    public void setMac(String mac) {
        this.mac = mac;
    }

    public int getRssi() {
        return rssi;
    }

    public void setRssi(int rssi) {
        this.rssi = rssi;
    }
}