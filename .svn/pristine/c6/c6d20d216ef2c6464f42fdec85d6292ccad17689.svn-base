<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>

<html>
<body>
<h1>
    <c:choose>
        <c:when test="${not empty buildingName}">
            Found building <c:out value="${buildingName}"/>
        </c:when>
        <c:otherwise>
            Didn't find any building.
        </c:otherwise>
    </c:choose>
</h1>
</body>
</html>
