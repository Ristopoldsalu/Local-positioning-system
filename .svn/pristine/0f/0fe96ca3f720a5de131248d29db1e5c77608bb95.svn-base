package ee.lps.model;

public class Room {
    private int roomId;
    private String roomName;
    private String roomNumber;
    private String roomPolygon;
    private int floorId;

    public Room(int roomId, String roomName, String roomNumber, String roomPolygon, int floorId) {
        this.roomId = roomId;
        this.roomName = roomName;
        this.roomNumber = roomNumber;
        this.roomPolygon = roomPolygon;
        this.floorId = floorId;
    }

    public Room() {}

    public int getRoomId() {
        return roomId;
    }

    public void setRoomId(int roomId) {
        this.roomId = roomId;
    }

    public String getRoomName() {
        return roomName;
    }

    public void setRoomName(String roomName) {
        this.roomName = roomName;
    }

    public String getRoomNumber() {
        return roomNumber;
    }

    public void setRoomNumber(String roomNumber) {
        this.roomNumber = roomNumber;
    }

    public String getRoomPolygon() {
        return roomPolygon;
    }

    public void setRoomPolygon(String roomPolygon) {
        this.roomPolygon = roomPolygon;
    }

    public int getFloorId() {
        return floorId;
    }

    public void setFloorId(int floorId) {
        this.floorId = floorId;
    }

    @Override
    public String toString() {
        return "Room{" +
                "roomId=" + roomId +
                ", roomName='" + roomName + '\'' +
                ", roomNumber='" + roomNumber + '\'' +
                ", roomPolygon='" + roomPolygon + '\'' +
                ", floorId=" + floorId +
                '}';
    }
}
